<Window x:Class="toto.apf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:toto.apf"
        mc:Ignorable="d"
        Title="MainWindow" Height="282" Width="428">
    <Grid Margin="0,0,0,9">
        <Label Content="Kérem a forduló eredményeit [1,2,x];" HorizontalAlignment="Left" Margin="20,46,0,0" VerticalAlignment="Top"/>
        <CheckBox x:Name="cbxHossz" Content="Nem megfelelő a karakterek száma(14)" HorizontalAlignment="Left" Margin="20,120,0,0" VerticalAlignment="Top" Checked="CheckBox_Checked" IsEnabled="False" Height="20" Width="245"/>
        <CheckBox x:Name="cbxKarakter" Content="Helytelen karakter az eredményekben()" HorizontalAlignment="Left" Margin="20,145,0,0" VerticalAlignment="Top" IsEnabled="False" Height="26" Width="233"/>
        <Button x:Name="btnMentes" Content="Eredmények mentése" HorizontalAlignment="Left" Margin="20,192,0,0" VerticalAlignment="Top" Width="233" Height="37"/>
        <TextBox HorizontalAlignment="Left" Margin="22,77,0,0" TextWrapping="Wrap" Text="12x12x12x12x12" VerticalAlignment="Top" Width="353" Height="25" FontSize="15" TextChanged="txbInput_TextChanged"/>

    </Grid>
</Window>
